<h3>POST COURSES</h3>
<form method="POST" action="/course/store">
  <div class="form-group">
    <label for="name">Name</label>
    <input
      type="text"
      class="form-control"
      id="name"
      name="name"
      placeholder="Name of video"
    />
  </div>
  <div class="form-group">
    <label for="description">Description</label>
    <input
      type="text"
      class="form-control"
      id="description"
      name="description"
      placeholder="Description of video"
    />
  </div>
  <div class="form-group">
    <label for="videoId">VideoId</label>
    <input
      type="text"
      class="form-control"
      id="videoId"
      name="videoId"
      placeholder="VideoId of video"
    />
  </div>
  <button type="submit" class="btn btn-primary">POST</button>
</form>